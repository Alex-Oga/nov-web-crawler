<h1>New Website</h1>

<%= form_with model: @website do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :url %>
    <%= form.text_field :url %>
  </div>

  <div>
    <%= form.submit "Create Website" %>
  </div>
<% end %>